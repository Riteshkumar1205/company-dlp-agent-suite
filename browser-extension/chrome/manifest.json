{
  "manifest_version": 3,
  "name": "Company Upload Detector",
  "version": "1.0",
  "permissions": ["scripting","activeTab"],
  "host_permissions": ["<all_urls>"],
  "background": {"service_worker": "background.js"},
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["content_script.js"],
    "run_at": "document_end"
  }]
}
